{
  "name": "rss-puzzle",
  "version": "1.0.0",
  "description": "Setup project",
  "main": "webpack.base.config.js",
  "scripts": {
    "start": "webpack serve --open --config ./webpack.config.js --env mode=dev",
    "build": "webpack",
    "lint": "eslint . --ext .ts",
    "format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "ci:format": "prettier --check 'src/**/*.ts'",
    "pre-push-eslint": "eslint --fix",
    "prepare": "husky",
    "deploy": "npm run build && npx gh-pages -d dist -e rss-puzzle"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "lint-staged": {
    "src/**/*.{ts, tsx, scss, css}": "npx prettier --write"
  },  
  "keywords": [],
  "author": "Aleh Serhiyenia",
  "license": "ISC",
  "homepage": "",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^12.0.2",
    "css-loader": "^6.10.0",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.2.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-webpack-plugin": "^4.0.1",
    "html-webpack-plugin": "^5.6.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "style-loader": "^3.3.4",
    "ts-loader": "^9.5.1",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.0",
    "webpack-merge": "^5.10.0"
  }
}
